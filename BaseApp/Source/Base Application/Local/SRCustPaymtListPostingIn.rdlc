  <Code>Public Function BlankZero(ByVal Value As Decimal)
    if Value = 0 then
        Return ""
    end if
    Return Value
End Function

Public Function ZeroBlank(ByVal Value As String)
    if Value = "" then
        Return 0
    end if
    Return Value
End Function

Public Function BlankPos(ByVal Value As Decimal)
    if Value &gt; 0 then
        Return ""
    end if
    Return Value
End Function

Public Function BlankZeroAndPos(ByVal Value As Decimal)
    if Value &gt;= 0 then
        Return ""
    end if
    Return Value
End Function

Public Function BlankNeg(ByVal Value As Decimal)
    if Value &lt; 0 then
        Return ""
    end if
    Return Value
End Function

Public Function BlankNegAndZero(ByVal Value As Decimal)
    if Value &lt;= 0 then
        Return ""
    end if
    Return Value
End Function

Shared CustPymLstCpt As String
Shared CompyName As String
Shared Text004GETFILTERS As String
Shared Text003SELECTSTR As String
Shared FMT_TODAY As String
Shared CRep_PGNOCap As String
Public Function SetCommonStr(ByVal CPLC As String, ByVal CN As String, ByVal T004GTFLT As String, ByVal T003SLTSTR As String, ByVal FMTTDY As String, ByVal CRPNC As String)
  CustPymLstCpt = CPLC
  CompyName = CN
  Text004GETFILTERS = T004GTFLT
  Text003SELECTSTR = T003SLTSTR
  FMT_TODAY = FMTTDY
  CRep_PGNOCap = CRPNC
End Function

Public Function GetCustPymLstCpt() As String
  Return CustPymLstCpt
End Function

Public Function GetCompyName() As String
  Return CompyName
End Function

Public Function GetText004GETFILTERS() As String
  Return Text004GETFILTERS
End Function

Public Function GetText003SELECTSTR() As String
  Return Text003SELECTSTR
End Function

Public Function GetFMT_TODAY() As String
  Return FMT_TODAY
End Function

Public Function GetCRep_PGNOCap() As String
  Return CRep_PGNOCap
End Function

Public TempPmtLCY As Decimal=0
Public SumPmtLCY As Decimal=0

Public TempPmtDiscLCY As Decimal=0
Public SumPmtDiscLCY As Decimal=0

Public TempAmountLCY As Decimal=0
Public SumAmountLCY As Decimal=0


Public Function Sum(ByVal PL As Decimal, ByVal PDL As Decimal, ByVal AL As Decimal, ByVal Index As Decimal)
  if Index = 1 then
    TempPmtLCY = TempPmtLCY + PL
    SumPmtLCY  =  SumPmtLCY + PL
    TempPmtDiscLCY = TempPmtDiscLCY + PDL
    SumPmtDiscLCY = SumPmtDiscLCY + PDL
    TempAmountLCY = TempAmountLCY + AL
    SumAmountLCY = SumAmountLCY + AL
  end if
End Function

Public Function GetSumPmtLCY(ByVal flag As Boolean) As Decimal
  if flag then
    Dim temp As Decimal
    temp = TempPmtLCY
    TempPmtLCY = 0
    Return temp
  end if
End Function

Public Function GetSumPmtDiscLCY(ByVal flag As Boolean) As Decimal
  if flag then
    Dim temp As Decimal
    temp = TempPmtDiscLCY
    TempPmtDiscLCY = 0
    Return temp
  end if
End Function

Public Function GetSumAmountLCY(ByVal flag As Boolean) As Decimal
  if flag then
    Dim temp As Decimal
    temp = TempAmountLCY
    TempAmountLCY = 0
    Return temp
  end if
End Function</Code>
